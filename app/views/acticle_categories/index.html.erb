<p id="notice"><%= notice %></p>

<h1>Listing Acticle Categories</h1>

<table>
  <thead>
    <tr>
      <th>Article</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @acticle_categories.each do |acticle_category| %>
      <tr>
        <td><%= acticle_category.article_id %></td>
        <td><%= acticle_category.category_id %></td>
        <td><%= link_to 'Show', acticle_category %></td>
        <td><%= link_to 'Edit', edit_acticle_category_path(acticle_category) %></td>
        <td><%= link_to 'Destroy', acticle_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Acticle category', new_acticle_category_path %>
